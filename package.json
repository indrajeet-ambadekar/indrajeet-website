{
  "name": "indrajeetambadekar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "watch": "onchange 'src/**/*.**' 'public/**/*.**' './package.json' -- prettier --write {{changed}}",
    "clean-s3": "aws s3 rm s3://indrajeetambadekar.elysium-cloud.com --recursive",
    "sync-s3": "aws s3 sync static/. s3://indrajeetambadekar.elysium-cloud.com",
    "invalidate":"aws cloudfront create-invalidation --distribution-id E2S068BLINK7GX --paths '/*'",
    "deploy": "npm run clean-s3 && npm run sync-s3 && npm run invalidate",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "onchange": "^7.1.0",
    "prettier": "^3.0.0"
  }
}
